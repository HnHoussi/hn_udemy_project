{% extends 'base.html.twig' %}
{% block title %}Welcome index!{% endblock %}
{% block body %}
    <div>
        {% if messages|length > 0 %}
            {%  for message in messages|slice(0, limit) %}
                <div> {{ message }}</div>
            {%  endfor %}
        {% else %}
            <div>Nothing is there</div>
        {% endif %}

    </div>
{% endblock %}

